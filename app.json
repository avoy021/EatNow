{
  "expo": {
    "name": "food_delivery_app",
    "slug": "food_delivery_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "fooddeliveryapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/images/icon.png",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#FF8C00"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.food_delivery_app"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FF8C00",
          "image": "./assets/images/splash01.png",
          "dark": {
            "image": "./assets/images/splash01.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ]
    ],
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
